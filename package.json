{
  "name": "forum-scraper",
  "version": "1.0.0",
  "description": "Discourse forum scraper for LLM training data",
  "main": "dist/index.js",
  "scripts": {
    "start": "ts-node src/index.ts",
    "scrape": "DEBUG=forum-scraper:* node scripts/scrape.js",
    "build": "tsc",
    "debug": "ts-node src/debug.ts",
    "test": "mocha test/**/*.test.js",
    "test:scraper": "mocha test/scraper.test.js",
    "test:api": "mocha test/api-integration.test.js",
    "test:watch": "mocha test/**/*.test.js --watch",
    "test:coverage": "nyc mocha test/**/*.test.js",
    "test:providers": "mocha test/providers.test.js",
    "test:integration": "mocha test/llm-integration.test.js"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.14.1",
    "axios": "^1.6.0",
    "body-parser": "^1.20.3",
    "cheerio": "^1.0.0-rc.12",
    "debug": "^4.4.0",
    "dotenv": "^16.4.1",
    "express": "^4.21.2",
    "html-entities": "^2.4.0",
    "openai": "^4.84.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "chai": "^4.5.0",
    "glob": "^10.0.0",
    "mocha": "^9.1.3",
    "nock": "^13.5.6",
    "nyc": "^15.1.0",
    "rimraf": "^5.0.0",
    "ts-node": "^10.9.0",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.0"
  }
}
